      "outputs": [],
      "source": [
        "def add_model_results(model_name, auroc, logloss):\n",
        "    global model_results\n",
        "    new_row = {\n",
        "        \"Model\": model_name,\n",
        "        \"AUROC\": round(auroc, 4),\n",
        "        \"Log Loss\": round(logloss, 4)\n",
        "    }\n",
        "    model_results = pd.concat([model_results, pd.DataFrame([new_row])], ignore_index=True)"
      ]
    },
